<div class="button-fixed-container-dark circle-icon-container <%= local_assigns[:class] %>">
    <div class="circle-icon-outer">
        <div class="flex-col circle-icon-left">
            <div class="circle-icon transparent">
                <%=link_to (image_tag 'github-icon.svg', alt: "github logo"), github_url, target: '_blank' %>
            </div>
        </div>
        <div class="flex-col circle-icon-right">
            <h2><%= link_to "View GitHub", github_url, target: '_blank' %></h2>
            <div class="buttons-desktop">
                <%= link_to "View My GitHub<i class='icon fa fa-chevron-right'></i>".html_safe, github_url, class: 'button-fixed circle-icon-button', target: '_blank' %>  
            </div>
        </div>
    </div>
    <%= link_to "View My GitHub<i class='icon fa fa-chevron-right'></i>".html_safe, github_url, class: 'button-fixed circle-icon-button', target: '_blank' %>  
</div> 